@model List<PointRecord.Models.Employees.Employees>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>
    Lista de Funcionários
</h1>

@Html.ActionLink("Novo Funcionário", "add", "employees")


<table>
    <tr>
        <th>Nome</th>
        <th>Cidade</th>
        <th>Telefone</th>
        <th>Estado</th>
        <th>Ações</th>

    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Name</td>
            <td>@item.City</td>
            <td>@item.Telephone</td>
            <td>@item._State</td>
         
            <td>
                <a asp-controller="employees" asp-action="update" asp-route-id="@item.Id"> Edit</a> |
                <a asp-controller="employees" asp-action="delete" asp-route-id="@item.Id"> Delete </a>

            </td>
        </tr>
    }
</table>